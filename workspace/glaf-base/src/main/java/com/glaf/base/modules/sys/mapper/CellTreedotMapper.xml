	<select id="getCellTreedotsByTopId" parameterType="string" resultMap="cellTreedotResultMap">
		
		select * from cell_treedot 
		where ( 
		    index_id in ( select index_id from cell_setmymenulst where topid=#{topid} ) 
		    or parent_id in ( select index_id from cell_setmymenulst where topid=#{topid} )
		) 
		order by parent_id,listno

	</select>
	
	<select id="getTopLevelCellTreedotsByTopId" parameterType="string" resultMap="cellTreedotResultMap">
		
		select * from cell_treedot 
		where index_id in ( select index_id from cell_setmymenulst where topid=#{topid} ) 
		and ( parent_id=-1 or parent_id=index_id ) 
		order by listno

	</select>
	
	<select id="getChildrenCellTreedots" parameterType="int" resultMap="cellTreedotResultMap">
		
	    select * from cell_treedot 
	    where parent_id=#{parentId} 
	    and index_id != parent_id 
	    and (num in('0','1','9') or issystem=0 or issystem=1) 
	    order by listno

	</select>
	

