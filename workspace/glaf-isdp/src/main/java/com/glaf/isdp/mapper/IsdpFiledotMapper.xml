	<select id="getFiledotsOfTreedot" parameterType="int" resultMap="filedotResultMap">
		
        select E.index_id,E.fileID,E.fbelong,E.fnum,E.pbelong,E.num,E.fname,E.dotname,E.ctime,
				E.dtime,E.file_name,E.filesize,E.dwid,E.fbid,E.fxid,E.jid,E.flid,E.topnode,
				E.cman,E.content,E.listflag,E.tofile,E.isink,E.dottype,E.ctimedname,
				E.type,E.listno,E.utree_index,E.isquan,E.intsysform
	    from filedot E where exists(
			 select * from cell_treedot_dot 
			 where index_id = #{indexId}
			 and filedot_fileid=E.fileid
		)
	</select>
	
	
	<select id="getFiledotsByCheckItemId" parameterType="string" resultType="com.glaf.isdp.domain.IsdpFiledot">
		
		select fileID as fileID, dotname as dotname, num as num from filedot where exists (
		    select * from cell_chkbatch_chkitem where proj_chkitem_id=#{proj_chkitem_id} 
		    and chkbatch_id=filedot.fileid
		)
		
	</select>
    

