			</if>

			<if test="fbids != null and fbids.size != 0">
			    and E.fbid IN
				<foreach item="x_fbid" index="index" collection="fbids" 
					open="(" separator="," close=")">
                  #{x_fbid}
				</foreach>
			</if>

			<if test="fxid != null">
