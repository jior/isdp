	<select id="getMyCellTaskFiles" parameterType="java.util.HashMap" resultMap="filedotResultMap2">
		 select a.fileid,a.dotname,a.num,b.inttasktype 
		 from filedot a, mycell_tasks_sub b 
		 where b.index_id=#{indexId}
		 and intistasks=#{intistasks}
		 and a.fileid=b.filedot_fileid
	</select>
	
	
