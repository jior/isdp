                  #{x_issystem}
				</foreach>
			</if>

	        
			<if test="fname != null and fname != '' ">
				and E.fname = #{fname}
			</if>

			<if test="fnameLike != null and fnameLike != '' ">
				and E.fname like #{fnameLike}
			</if>

			<if test="fnames != null and fnames.size() &gt; 0">
			    and E.fname IN
