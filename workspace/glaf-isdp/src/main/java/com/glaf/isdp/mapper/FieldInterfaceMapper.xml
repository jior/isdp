					open="(" separator="," close=")">
                  #{x_dtype}
				</foreach>
			</if>

	        
			<if test="showtype != null and showtype != '' ">
				and E.showtype = #{showtype}
			</if>

			<if test="showtypeLike != null and showtypeLike != '' ">
				and E.showtype like #{showtypeLike}
			</if>

			<if test="showtypes != null and showtypes.size() &gt; 0">
