	<delete id="deleteFieldInterfaceById" parameterType="string"> 
        delete from interface
        where listId = #{id}
	</delete>
	

	<delete id="deleteFieldInterfaces" parameterType="com.glaf.isdp.query.FieldInterfaceQuery">
		delete from interface
		where ( listId IN
		<foreach item="x_rowId" index="index" collection="rowIds" open="(" separator="," close=")">
              #{x_rowId}
		</foreach>
		)
	</delete>


	<select id="getFieldInterfaceById" parameterType="string" resultMap="fieldInterfaceResultMap">
		select * from interface where listId = #{id}
	</select>
	
	
	<select id="getListShowFieldsByFrmType" parameterType="string" resultMap="fieldInterfaceResultMap">
		
		select * from interface a,listshow b 
		where a.frmtype=#{frmType}  
		and a.listid=b.listid  
		order by b.listno

	</select>
	
	<select id="getListShowFields" parameterType="map" resultMap="fieldInterfaceResultMap">
		
		select  distinct * from interface b,listshow a 
		where a.listid=b.listid 
		and a.index_id=#{indexId}
		and a.frmtype=#{frmType}  
		order by a.listno

	</select>
	
	<select id="getFieldsByFrmType" parameterType="string" resultMap="fieldInterfaceResultMap">
		
		select * from interface  
		where frmtype=#{frmType} 
		order by listno

	</select>


	<select id="getFieldInterfaceCount" 
		parameterType="map"
		resultType="int">
		select count(*)
		<include refid="selectFieldInterfacesSql" />
	</select>


	<select id="getFieldInterfaceCountByQueryCriteria" 
		parameterType="com.glaf.isdp.query.FieldInterfaceQuery"
		resultType="int">
		select count(*)
		<include refid="selectFieldInterfacesSql" />
	</select>


	<select id="getFieldInterfaces" 
		parameterType="map"
		resultMap="fieldInterfaceResultMap">
		select E.*
		<include refid="selectFieldInterfacesSql" />
		
		<choose>
			<when test="orderBy != null">
			     order by ${orderBy}
			</when>
			<otherwise>
				 order by E.listno
			</otherwise>
		</choose>
		
	</select>

	<select id="getFieldInterfacesByQueryCriteria" 
		parameterType="com.glaf.isdp.query.FieldInterfaceQuery"
		resultMap="fieldInterfaceResultMap">
		
		select E.*
		<include refid="selectFieldInterfacesSql" />
		
		<choose>
			<when test="orderBy != null">
			     order by ${orderBy}
			</when>
			<otherwise>
				 order by E.listno
			</otherwise>
		</choose>
		
	</select>


	<sql id="selectFieldInterfacesSql">

		from interface E
		 
		<where>

		       1 = 1  
	        
			<if test="indexId != null and indexId != '' ">
				and E.index_id = #{indexId}
			</if>

			<if test="indexIdLike != null and indexIdLike != '' ">
				and E.index_id like #{indexIdLike}
			</if>

			<if test="indexIds != null and indexIds.size() &gt; 0">
			    and E.index_id IN
				<foreach item="x_indexId" index="index" collection="indexIds" 
					open="(" separator="," close=")">
                  #{x_indexId}
				</foreach>
			</if>

	        
			<if test="frmtype != null and frmtype != '' ">
				and E.frmtype = #{frmtype}
			</if>

			<if test="frmtypeLike != null and frmtypeLike != '' ">
				and E.frmtype like #{frmtypeLike}
			</if>

			<if test="frmtypes != null and frmtypes.size() &gt; 0">
			    and E.frmtype IN
				<foreach item="x_frmtype" index="index" collection="frmtypes" 
					open="(" separator="," close=")">
                  #{x_frmtype}
				</foreach>
			</if>

	        
			<if test="issystem != null and issystem != '' ">
				and E.issystem = #{issystem}
			</if>

			<if test="issystemLike != null and issystemLike != '' ">
				and E.issystem like #{issystemLike}
			</if>

			<if test="issystems != null and issystems.size() &gt; 0">
			    and E.issystem IN
				<foreach item="x_issystem" index="index" collection="issystems" 
